#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
    string a,b;
    cin>>a;
    cin>>b;
    int x = a.size();
    int y = b.size();
    cout<<x<<" "<<y<<endl;
    string c = a+b;
    cout<<c<<endl;
    char d = a[0];
    char e = b[0];
    a[0] = e;
    b[0] = d;
    cout<<a<<" "<<b;      
    return 0;
}
